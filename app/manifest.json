{
  "name": "Save to querySelector",
  "version": "1.0",
  "description": "Save content to queryselector.com",
  "permissions": ["activeTab","storage","declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./icon.png",
      "32": "./icon.png",
      "48": "./icon.png",
      "128": "./icon.png"
    }
  },
  "icons": {
    "16": "./icon.png",
    "32": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  },

  "manifest_version": 2
}
